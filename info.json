{
  "id": "plugin.coulsontl.translate.chatgpt",
  "plugin_type": "translate",
  "icon": "openai.svg",
  "display": "ChatGPT",
  "homepage": "https://github.com/coulsontl/pot-app-translate-plugin-chatgpt",
  "needs": [
    {
      "key": "model",
      "display": "模型",
      "type": "input"
    },
    {
      "key": "requestPath",
      "display": "请求路径",
      "type": "input"
    },
    {
      "key": "apiKey",
      "display": "API Key",
      "type": "input"
    },
    {
        "key": "useStream",
        "display": "流式输出",
        "type": "select",
        "options": {
            "true": "是 / Yes",
            "false": "否 / No"
        }
    },
    {
      "key": "systemPrompt",
      "display": "System Prompt",
      "type": "input"
    },
    {
      "key": "userPrompt",
      "display": "User Prompt",
      "type": "input"
    },
    {
      "key": "requestArguments",
      "display": "Request Arguments",
      "type": "input"
    }
